
  {
    "manifest_version": 3,
    "name": "Minimal Manifest",
    "version": "1.0.0",
    "description": "A basic example extension with only required keys",
    "icons": {
        "21": "icon21.png"
      },
    "backgroud": {
        "service_worker":"service-worker.js",
        "type": "module"
    },
    "action":{ 
        "default_popup" : "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permission": [
        "http://*/*",
        "http://*/*"
    ]
  }